@model UserAdminViewModel
@{
    ViewBag.Title = "UserAdminEditUser";
    Layout = "_Layout";
}
<div>
    <h1>Test tekst for EditUser Useradmin</h1>

    <div>
        <form asp-action="EditUser">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="CurrentLaundryUser"/>
            <div>
                <label>Name of the user </label>
                @Html.DisplayFor(model=>model.CurrentLaundryUser.Name)
                <span asp-validation-for="CurrentLaundryUser" class="text-danger"></span>
            </div>
        </form>

    </div>

    <aside>
        <figure>
            <img src="/Images/logo.jfif" style="float: right; width: 225px; height: 225px" alt="Laundry time logo">
        </figure>

    </aside>
</div>
