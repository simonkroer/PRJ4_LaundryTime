@model SystemAdminViewModel

@{
	ViewData["Title"] = "Machine Index";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="column left">

	<img height="300" src="Images/logo.png" />

	<div class="headerCSS">
		<p style="font-size: 21px">All User Admins:</p>
		<a class="btn btnColor raise " type="button" asp-action="CreateUserAdmin" >Add UserAdmin</a>
	</div>

	<div class="p-4 border rounded">
		<table id="tblData" class="table table-striped table-bordered" style="width: 100%">
			<thead class="thead-dark">
			<tr class="table-info">
				<th  style="font-size: 21px">Name:</th>

			</tr>
			</thead>
			<tbody>
			@foreach (UserAdmin item in Model.AllUserAdmins)
			{
				<tr>
					<td  style="font-size: 21px">@item.Name</td>
				</tr>
			}
			</tbody>
		</table>
	</div>
</div>
