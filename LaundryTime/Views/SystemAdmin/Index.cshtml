@model SystemAdminViewModel

@{
    ViewData["Title"] = "Machine Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="column left">

    <img height="300" src="Images/logo.png" />

    <div class="headerCSS">
        <p style="font-size: 21px">All User Admins:</p>
        <a class="btn btnColor raise " type="button" asp-action="CreateUserAdmin">Add UserAdmin</a>
    </div>

    <div class="p-4 border rounded">
        <table id="tblData" class="table table-striped table-bordered" style="width: 100%">
            <thead class="thead-dark">
                <tr class="table-info">
                    <th>
                        @Html.DisplayNameFor(model => model.UserAdmin.)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.CheckedIn)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.GuestName)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.RoomNumber)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.NoOfChildren)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.NoOfAdults)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (UserAdmin item in Model.AllUserAdmins)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.DateTime)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CheckedIn)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.GuestName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.RoomNumber)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.NoOfChildren)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.NoOfAdults)
                        </td>
                        <td>
                            <a asp-action="Edit" asp-route-id="@item.BreakfastId">Edit</a> |
                            <a asp-action="Details" asp-route-id="@item.BreakfastId">Details</a> |
                            <a asp-action="Delete" asp-route-id="@item.BreakfastId">Delete</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>

    </div>
</div>


    
        
